@startuml

start
:Edit command is parsed;

if () then ([else])
  :Parse prefixes;
  if () then ([else])
    :Execute Edit command;
    if () is ([else]) then
      if () is ([else]) then
        :Edit person successfully;
      else ([email is edited to a duplicate])
        :Reject;
      endif
